{
    "name": "joselfonseca/laravel-api-tools",
    "description": "Some tools for Laravel API development, nothing really new, just warpers and helper stuff",
    "authors": [
        {
            "name": "Jose Luis Fonseca",
            "email": "jose@ditecnologia.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "lucadegasperi/oauth2-server-laravel": "3.*",
        "league/fractal": "0.9.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Joselfonseca\\LaravelApiTools\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
