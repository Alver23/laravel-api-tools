{
    "name": "joselfonseca/laravel-api-tools",
    "description": "Some tools for Laravel API development, nothing really new, just wrapers and helper stuff",
    "license": "MIT",    
    "authors": [
        {
            "name": "Jose Luis Fonseca",
            "email": "jose@ditecnologia.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "tymon/jwt-auth": "0.5.*",
        "dingo/api" : "1.0.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.0",
        "orchestra/testbench": "~3.1",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Joselfonseca\\LaravelApiTools\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Joselfonseca\\LaravelApiTools\\Tests\\": "tests/"
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    }
}
